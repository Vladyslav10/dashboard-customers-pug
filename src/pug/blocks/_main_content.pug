.main__content
	button(class="main__back" type="button") back
	h2.main__title Hello Evano üëãüèº,
	.main__body
		.main__top
			.main__content-info
				h3.main__choosen-element All Customers
				p.main__members Active Members
			.main__input
				input(type="text" placeholder="Search")
		.main__users
			ul.main__list
				li.main__user-info.main__user-info-headline
					p.main__customer-name Customer Name
					p.main__company Company
					p.main__phone Phone Number
					p.main__email Email
					p.main__country Country
					p.main__status Status
				each user in users_array
					li.main__user-info
						p.main__customer-name= user.name
						p.main__company= user.company
						p.main__phone= user.phone
						p.main__email= user.email
						p.main__country= user.country
						p(class=`main__status ${user.status.toLowerCase()}`)= user.status
		.main__bottom
			p.main__pagination-info Showing data 1 to 8 of 256K entries
			.main__pagination
