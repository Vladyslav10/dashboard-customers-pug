.main__content
	.menu_trigger.menuTrigger
		span.menu_trigger_decor
	h2.main__title Hello Evano üëãüèº,
	.main__body
		.main__top
			.main__text
				h3.main__customers All Customers
				p.main__members Active Members
			form.main__form
				+form_input({type: 'text',label: '',placeholder: 'Search',value: '',id: 'input_1',})
					button(type='submit', class='main__button') 
						+icon('search')
		.main__users
			.main__user.main__user--headline
				.main__name Customer Name
				.main__company Company
				.main__phone Phone Number
				.main__email Email
				.main__country Country
				.main__status Status
			each user in users_array
				.main__user
					.main__name= user.name
					.main__company= user.company
					a.main__phone(href=`tel:${user.phone}`)= user.phone
					a.main__email(href=`mailto:${user.email}`)= user.email
					.main__country= user.country
					div(class=`main__status main__status--${user.status.toLowerCase()}`)= user.status
		.main__bottom
			p.main__number Showing data 1 to 8 of 256K entries
			.main__pagination
				button.main__pagination-arrow.prev <
				ul.main__pagination-items
				button.main__pagination-arrow.next >